{
  "name": "flare",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "debug": "open rndebugger://set-debugger-loc?host=localhost&port=8081"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add -A",
      "npm run lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/react-native-permissions": "^1.1.1",
    "@types/react-native-vector-icons": "^6.4.0",
    "@types/react-redux": "^7.0.8",
    "@types/remote-redux-devtools": "^0.5.3",
    "@types/styled-components": "^4.1.14",
    "react": "16.8.3",
    "react-native": "0.59.5",
    "react-native-android-open-settings": "^1.3.0",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-maps": "^0.24.2",
    "react-native-permissions": "^1.1.1",
    "react-native-spinkit": "^1.2.0",
    "react-native-textinput-effects": "^0.5.1",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^3.9.1",
    "react-navigation-transitions": "^1.0.10",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/runtime": "7.4.4",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.14",
    "@types/react-native": "0.57.50",
    "@types/react-test-renderer": "16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-jest": "24.7.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.1.0",
    "jest": "24.7.1",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.53.1",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.8.3",
    "remote-redux-devtools": "^0.5.16",
    "ts-jest": "^24.0.2",
    "typescript": "3.4.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
